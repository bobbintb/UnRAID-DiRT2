{
  "name": "dirt-backend",
  "version": "1.0.0",
  "description": "Backend WebSocket server for the DIRT Unraid plugin.",
  "main": "dirt.js",
  "scripts": {
    "start": "node nodejs/dirt.js >> ../dirt_server.log 2>&1 &",
    "stop": "ps aux | grep 'dirt.js' | grep -v grep | awk '{print $2}' | xargs kill -9",
    "restart": "npm run stop && npm run start",
    "seed": "node tests/helpers/seed-redis.helper.js",
    "unraid-workaround": "mkdir -p jules-scratch/verification && tail -n +5 dirt-tabulator.page > jules-scratch/verification/dirt-tabulator.php && sed -i '1i\\<script src=\"https://code.jquery.com/jquery-3.7.1.min.js\"></script>' jules-scratch/verification/dirt-tabulator.php"
    "clean": "rm -rf dirt_server.log jules-scratch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "blake3": "^3.0.0",
    "bullmq": "^5.7.8",
    "redis": "^4.6.13",
    "redis-om": "^0.4.7",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.44.1"
  },
  "overrides": {
    "blake3-wasm": "3.0.0",
    "@c4312/blake3-internal": "3.0.0"
  }
}
