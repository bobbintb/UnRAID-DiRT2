{
  "name": "dirt-backend",
  "version": "1.0.0",
  "description": "Backend WebSocket server for the DIRT Unraid plugin.",
  "main": "dirt.js",
  "scripts": {
    "start": "cd nodejs && node dirt.js >> ../dirt_server.log 2>&1 &",
    "seed": "cd nodejs && node ../tests/helpers/seed-redis.helper.js",
    "unraid-workaround": "mkdir -p jules-scratch/verification && tail -n +4 dirt-tabulator.page | sed 's/window.location.hostname/\"localhost\"/' > jules-scratch/verification/temp_tabulator.html",
    "clean": "rm -rf dirt_server.log jules-scratch",
    "kill-dirt": "ps aux | grep 'dirt.js' | grep -v grep | awk '{print $2}' | xargs kill -9"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "blake3": "^3.0.0",
    "bullmq": "^5.7.8",
    "redis": "^4.6.13",
    "redis-om": "^0.4.7",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.44.1"
  },
  "overrides": {
    "blake3-wasm": "3.0.0",
    "@c4312/blake3-internal": "3.0.0"
  }
}
